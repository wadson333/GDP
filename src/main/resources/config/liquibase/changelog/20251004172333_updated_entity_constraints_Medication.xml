    <!--
        Added the field constraints for entity Medication.
    -->
    <changeSet id="20251004172333-fields-constraints" author="jhipster">
        <addNotNullConstraint tableName="medication"
            columnName="route_of_administration"
            columnDataType="varchar(255)"/>
